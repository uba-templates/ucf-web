{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        "dynamic-import-webpack",
        "@babel/plugin-proposal-class-properties",
        ["@babel/plugin-transform-runtime", {
            "corejs": false,
            "helpers": true,
            "regenerator": true,
            "useESModules": false
        }]
    ]
}